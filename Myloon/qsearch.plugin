[Rewrite]
^https:\/\/duckduckgo\.com\/\?q=b\+([^&]+).+ 302 https://www.baidu.com/s?wd=$1
^https:\/\/duckduckgo\.com\/\?q=f\+([^&]+).+ 302 https://dict.youdao.com/search?q=$1
^https:\/\/duckduckgo\.com\/\?q=([^&]+).+ 302 https://www.google.com/search?q=$1

[MitM]
hostname = duckduckgo.com
