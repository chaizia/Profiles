#!name=Panels@柴
#!desc=FlushDNS、节点信息、流媒体解锁检测

[Panel]
#FlushDNS
SurgePro_FlushDNS=script-name=SurgePro_FlushDNS,update-interval=-1
#节点信息
Netinfo=script-name=Netinfo,title="𝗡𝗘𝗧𝗜𝗡𝗙𝗢",content="请刷新",style=info,update-interval=-1
#奈飞油管检测
stream-checktool=script-name=stream-check,title="𝗦𝗧𝗥𝗘𝗔𝗠 𝗖𝗛𝗘𝗖𝗞",content="请刷新面板",update-interval=-1

[Script]
#FlushDNS
SurgePro_FlushDNS=type=generic,timeout=10,script-path=https://raw.githubusercontent.com/chaizia/Profiles/master/MySurge/surgepro_flushdns.js,argument=icon=power.circle&color=#DC143C
#节点信息
Netinfo=type=generic,timeout=3,script-path=https://raw.githubusercontent.com/chaizia/Profiles/master/MySurge/ipcheck.js
#奈飞油管解锁检测
stream-check=type=generic,timeout=15,script-path=https://raw.githubusercontent.com/chaizia/Profiles/master/MySurge/stream-check2.js
